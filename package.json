    "@sveltejs/kit": "^1.0.0-next.266",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.15",
    "cytosis": "^0.0.5",
    "mdsvex": "^0.9.8",
    "plasmid": "git+https://github.com/janzheng/plasmid.git",
    "postcss": "^8.4.5",
